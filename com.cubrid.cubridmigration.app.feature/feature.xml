<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.cubrid.cubridmigration.app.feature"
      label="CUBRID Migration Toolkit Application"
      version="1.0.0.qualifier"
      provider-name="%providerName"
      plugin="com.cubrid.cubridmigration.app">

   <description url="http://www.cubrid.org">
      %description
   </description>

   <copyright url="http://www.cubrid.org">
      %copyright
   </copyright>

   <license url="http://www.cubrid.org">
      %licence
   </license>

   <url>
      <update label="CUBRID Migration Toolkit" url="http://ftp.cubrid.org/sites/rcp/cubridmigration/"/>
   </url>

   <includes
         id="org.eclipse.rcp"
         version="0.0.0"/>

   <requires>
      <import plugin="org.eclipse.core.runtime" version="0.0.0" match="compatible"/>
      <import plugin="com.cubrid.common.configuration" version="0.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui" version="0.0.0" match="compatible"/>
      <import plugin="org.eclipse.equinox.p2.ui" version="0.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jface" version="0.0.0" match="compatible"/>
      <import plugin="com.ibm.icu"/>
      <import plugin="org.eclipse.osgi"/>
   </requires>

   <plugin
         id="com.cubrid.cubridmigration.core"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.cubrid.cubridmigration.ui"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.cubrid.cubridmigration.app"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.ui.forms"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="com.cubrid.common.configuration"
         version="0.0.0"
         unpack="false"/>

</feature>
